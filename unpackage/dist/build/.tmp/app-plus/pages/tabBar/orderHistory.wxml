<view><view><uni-search-bar vue-id="1" placeholder="输入姓名/手机号/店名" value="{{searchValue}}" data-event-opts="{{[['^input',[['__set_model',['','searchValue','$event',[]]],['search']]]]}}" bind:input="__e" bind:__l="__l"></uni-search-bar></view><view class="Index-Page _section"><s-tabs vue-id="2" effect="{{true}}" nav-per-view="{{2}}" value="{{activeTab}}" data-event-opts="{{[['^change',[['change']]],['^input',[['__set_model',['','activeTab','$event',[]]]]]]}}" bind:change="__e" bind:input="__e" bind:__l="__l" vue-slots="{{['default']}}"><s-tab vue-id="{{('3')+','+('2')}}" title="派送中" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="lists"><view class="list"><view class="list-top"><view><text class="number-text">{{item.number+"件"}}</text></view><view><button type="primary" size="mini" data-event-opts="{{[['tap',[['send']]]]}}" bindtap="__e">确认派送</button></view></view><view data-event-opts="{{[['tap',[['goDeatalis',['$0'],[[['list','',index,'number']]]]]]]}}" bindtap="__e"><view><uni-icons class="ma-right" vue-id="{{('4-'+index)+','+('3')}}" type="person-filled" size="13" bind:__l="__l"></uni-icons><text class="ma-right">{{item.username}}</text><text>{{item.phone}}</text></view><view><uni-icons class="ma-right" vue-id="{{('5-'+index)+','+('3')}}" type="home" size="13" bind:__l="__l"></uni-icons><text>{{item.home}}</text></view><view><uni-icons class="ma-right" vue-id="{{('6-'+index)+','+('3')}}" type="location-filled" size="13" bind:__l="__l"></uni-icons><text>{{item.addres}}</text></view></view><view class="footer"><view class="footer-left"><text class="footer-text">{{"订单号: "+item.order}}</text></view><view class="footer-right"><view data-event-opts="{{[['tap',[['retention',['$event']]]]]}}" bindtap="__e"><uni-icons class="ma-right" vue-id="{{('7-'+index)+','+('3')}}" type="redo" size="11" bind:__l="__l"></uni-icons><text>滞留</text></view><view><uni-icons class="ma-right" vue-id="{{('8-