<view><view><uni-search-bar vue-id="1" placeholder="输入姓名/手机号/店名" value="{{searchValue}}" data-event-opts="{{[['^input',[['__set_model',['','searchValue','$event',[]]],['search']]]]}}" bind:input="__e" bind:__l="__l"></uni-search-bar></view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="lists"><view class="list"><view class="list-top"><view><text class="number-text">{{item.Many+"件"}}</text></view><view><button type="primary" size="mini" data-event-opts="{{[['tap',[['send',['$0'],[[['list','',index,'ID']]]]]]]}}" bindtap="__e">确认派送</button></view></view><view data-event-opts="{{[['tap',[['goDeatalis',['$0'],[[['list','',index,'ID']]]]]]]}}" bindtap="__e"><view><uni-icons class="ma-right" style="color:#9c9c9c;" vue-id="{{'2-'+index}}" type="person-filled" size="13" bind:__l="__l"></uni-icons><text class="ma-right">{{item.ClientName}}</text><text>{{item.PhoneNumber}}</text></view><view><uni-icons class="ma-right" style="color:#9c9c9c;" vue-id="{{'3-'+index}}" type="home" size="13" bind:__l="__l"></uni-icons><text>{{item.Shop}}</text></view><view><uni-icons class="ma-right" style="color:#9c9c9c;" vue-id="{{'4-'+index}}" type="location-filled" size="13" bind:__l="__l"></uni-icons><text>{{item.Address}}</text></view></view><view class="footer"><view class="footer-left"><text class="footer-text">{{"订单号: "+item.ID}}</text></view><view class="footer-right"><view data-event-opts="{{[['tap',[['deleteOrder',['$0'],[[['list','',index,'ID']]]]]]]}}" bindtap="__e"><uni-icons class="ma-right" vue-id="{{'5-'+index}}" type="trash" size="11" bind:__l="__l"></uni-icons><text style="font-size:20px;">删除</text></view></view></view></view></view></block></view>