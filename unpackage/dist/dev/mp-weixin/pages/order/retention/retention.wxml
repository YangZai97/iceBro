<view><view><uni-search-bar vue-id="1" placeholder="输入姓名/手机号/店名" value="{{searchValue}}" data-event-opts="{{[['^input',[['__set_model',['','searchValue','$event',[]]],['search']]]]}}" bind:input="__e" bind:__l="__l"></uni-search-bar></view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="lists"><view class="list"><view class="list-top"><view><text class="number-text">{{item.number+"件"}}</text></view><view><button type="primary" size="mini" data-event-opts="{{[['tap',[['send']]]]}}" bindtap="__e">确认派送</button></view></view><view data-event-opts="{{[['tap',[['goDeatalis',['$0'],[[['list','',index,'number']]]]]]]}}" bindtap="__e"><view><uni-icons class="ma-right" vue-id="{{'2-'+index}}" type="person-filled" size="13" bind:__l="__l"></uni-icons><text class="ma-right">{{item.username}}</text><text>{{item.phone}}</text></view><view><uni-icons class="ma-right" vue-id="{{'3-'+index}}" type="home" size="13" bind:__l="__l"></uni-icons><text>{{item.home}}</text></view><view><uni-icons class="ma-right" vue-id="{{'4-'+index}}" type="location-filled" size="13" bind:__l="__l"></uni-icons><text>{{item.addres}}</text></view></view><view class="footer"><view class="footer-left"><text class="footer-text">{{"订单号: "+item.order}}</text></view><view class="footer-right"><view><uni-icons class="ma-right" vue-id="{{'5-'+index}}" type="trash" size="11" bind:__l="__l"></uni-icons><text>删除</text></view></view></view></view></view></block></view>